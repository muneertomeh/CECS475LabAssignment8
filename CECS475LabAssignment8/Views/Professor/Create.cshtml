@model CECS475LabAssignment8.Models.Professor
@{ 
    Layout = "_Layout";

}

<h1>Add Professor to Professor Lists</h1>


<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
                <p></p>
            </div>
            <div class="form-group">
                <label asp-for="ProfessorID" class="control-label"></label>
                <input asp-for="ProfessorID" class="form-control" />
                <span asp-validation-for="ProfessorID" class="text-danger"></span>
                <p></p>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>