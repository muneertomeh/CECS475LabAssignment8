@model CECS475LabAssignment8.Models.Professor
@{ 
    Layout = "_Layout";

}

<h1>Add Professor to Professor Lists</h1>

@Html.BeginForm(){ 
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                @Html.LabelFor(professor => professor.ProfessorID);
                @Html.EditorFor(professor => professor.ProfessorID);
            </div>
            <div asp-validation-summary="ModelOnly" class="text-danger">
            </div>
            <div class="form_group">
                @Html.LabelFor(professor => professor.Name)
                @Html.EditorFor(professor => professor.Name)
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

}
