@model IEnumerable<CECS475LabAssignment8.Models.Professor>
@{ 
    Layout = "_Layout";
}
<h1> Professor</h1>


<p>
    Below are some of the latest reviews and ratings for our professors 
</p>




<div class="blog-posts">
    
    @Html.ActionLink("Create", "Create");

    @foreach (var professor in Model)
    {
        <table>
            <tr>
                @Html.Partial("_Post", professor);
                <td>
                    @Html.ActionLink("Reviews", "Reviews", "Professor", new { id = professor.ProfessorID })
                    @Html.ActionLink("AddReview", "AddReview", "Professor", new { id = professor.ProfessorID }) |

                </td>
            </tr>


        </table>




    }
</div>